{
  "name": "youtube-outlier-discovery",
  "version": "1.0.0",
  "description": "Intelligent web application for discovering high-performing YouTube videos from adjacent channels",
  "main": "server/index.js",
  "scripts": {
    "dev": "bun scripts/dev.js",
    "server:dev": "cd server && bun run dev",
    "client:dev": "cd client && bun run dev",
    "build": "cd client && bun run build",
    "start": "cd server && bun start",
    "install:all": "bun install && cd client && bun install && cd ../server && bun install",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "test:server": "bun test server/src/**/*.test.js --coverage",
    "test:client": "cd client && bun test --coverage",
    "test:integration": "bun test test/integration/**/*.test.js --coverage",
    "test:unit": "bun run test:server && bun run test:client",
    "test:config": "bun test --coverage --coverage-reporter text",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:all": "npm run lint && cd client && npm run lint && cd ../server && npm run lint",
    "lint:fix": "npm run lint -- --fix && cd client && npm run lint -- --fix && cd ../server && npm run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "keywords": [
    "youtube",
    "analytics",
    "content-creation",
    "outlier-detection",
    "video-analysis",
    "trend-discovery"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtube-outlier-discovery.git"
  }
}